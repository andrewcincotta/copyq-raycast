{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "copyq-clipboard-manager",
  "title": "CopyQ Clipboard Manager",
  "description": "Allow's the user to view and select clipboard contents from CopyQ",
  "icon": "command-icon.png",
  "author": "andrewcincotta",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "CopyQ",
      "description": "Show's Items in CopyQ Clipboard",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.60.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "18.18.4",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}